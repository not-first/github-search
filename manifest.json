{
  "manifest_version": 3,
  "name": "Github Search",
  "version": "1.0",
  "description": "Quickly search Github personal, starred or recently accessed repositories.",
  "permissions": [
    "storage",
    "alarms",
    "history"
  ],
  "icons": {
    "16": "icons/icon128.png",
    "32": "icons/icon128.png",
    "48": "icons/icon128.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon128.png",
      "32": "icons/icon128.png",
      "48": "icons/icon128.png",
      "128": "icons/icon128.png"
    }
  },
  "omnibox": {
    "keyword": "gh"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}